<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.UserMapper">
    <select id="findByUsername" resultType="com.vo.User" >
    	select * from brew_user where username=#{username}
    </select>
  	
  	<insert id="saveRegistUser" parameterType="com.vo.User">
  		insert into brew_user (username,password,email,active_code,regist_time,user_role,user_status) values 
  		(#{username},#{password},#{email},#{active_code},#{regist_time},#{user_role},#{user_status})
  	</insert>
    
</mapper>





